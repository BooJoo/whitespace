<?xml version="1.0" encoding="UTF-8"?>
<trigger-vocabularies>
    <vocabulary display-prefix="Tankstand unter" display-name="Tankstand" trigger-id="TankLevel">
        <arg
            name="Threshold"
            description="Tankstandssschwellwert"
            type="de.fuberlin.whitespace.regelbau.logic.data.PreselectedTriggerArgument"
			selector="de.fuberlin.whitespace.regelbau.MyNumberPicker" >
			
			    <operators />
			    <units>
			        <unit>%</unit>
			    </units>
				<values type="Integer">
					<value>10</value>
					<value>20</value>
					<value>30</value>
					<value>40</value>
					<value>50</value>
					<value>60</value>
					<value>70</value>
					<value>80</value>
					<value>90</value>
				</values>
			</arg>
    </vocabulary>
    <vocabulary display-prefix="Verbrauch" display-name="Kraftstoffverbrauch" trigger-id="FuelConsumption">
        <arg
            name="Threshold" 
            type="de.fuberlin.whitespace.regelbau.logic.data.PreselectedTriggerArgument"
            description="Verbrauchssschwellwert"
            selector="de.fuberlin.whitespace.regelbau.MySlotmachine" >
				
			    <operators>
			        <operator>gt</operator>
			        <operator>lt</operator>
			    </operators>
			    <units>
			        <unit>l/100km</unit>
			        <unit>l/h</unit>
			    </units>
				<values type="Integer">
					<value>3</value>
					<value>4</value>
					<value>5</value>
					<value>6</value>
					<value>7</value>
					<value>8</value>
					<value>9</value>
					<value>10</value>
					<value>12</value>
					<value>13</value>
					<value>14</value>
					<value>15</value>
					<value>20</value>
					<value>25</value>
					<value>30</value>
					<value>40</value>
					<value>50</value>
				</values>
			</arg>
    </vocabulary>
    <vocabulary display-prefix="nach" display-name="nach ... km" trigger-id="DrivingDistance">
        <arg 
            name="Threshold"
            type="de.fuberlin.whitespace.regelbau.logic.data.PreselectedTriggerArgument"
            description="Distanzschwellwert"
            selector="de.fuberlin.whitespace.regelbau.MySlotmachine" >
				
			    <operators />
			    <units>
			        <unit>km</unit>
			        <unit>miles</unit>
			    </units>
				<values type="Float">
					<value>10</value>
					<value>25</value>
					<value>50</value>
					<value>100</value>
					<value>200</value>
					<value>500</value>
				</values>
			</arg>
    </vocabulary>
    <vocabulary display-prefix="Temp" display-name="Geschwindigkeit" trigger-id="Speed">
        <arg 
            name="Threshold" 
            type="de.fuberlin.whitespace.regelbau.logic.data.PreselectedTriggerArgument" 
            description="Geschwindigkeitsschwellwert"
            selector="de.fuberlin.whitespace.regelbau.MySlotmachine" >
				
			    <operators>
			        <operator>gt</operator>
			        <operator>lt</operator>
			    </operators>
			    <units>
			        <unit>km/h</unit>
			        <unit>m/h</unit>
			    </units>
				<values type="Float">
					<value>10</value>
					<value>30</value>
					<value>50</value>
					<value>80</value>
					<value>100</value>
					<value>120</value>
				</values>
			</arg>
    </vocabulary>
	<vocabulary display-prefix="innen" display-name="Innentemperatur" trigger-id="InteriorTemperature">
        <arg 
            name="Threshold"
            type="de.fuberlin.whitespace.regelbau.logic.data.PreselectedTriggerArgument"
            description="Temperaturschwellwert"
            selector="de.fuberlin.whitespace.regelbau.MySlotmachine" >
				
			    <operators>
			        <operator>gt</operator>
			        <operator>lt</operator>
			    </operators>
			    <units>
			        <unit>°C</unit>
			        <unit>°F</unit>
			    </units>
				<values type="Integer">
				    <value>0</value>
				    <value>5</value>
					<value>10</value>
					<value>15</value>
					<value>20</value>
					<value>25</value>
					<value>30</value>
				</values>
			</arg>
	</vocabulary>
	<vocabulary display-prefix="außen" display-name="Außentemperatur" trigger-id="ExteriorTemperature">
        <arg 
            name="Threshold"
            type="de.fuberlin.whitespace.regelbau.logic.data.PreselectedTriggerArgument"
            description="Temperaturschwellwert"
            selector="de.fuberlin.whitespace.regelbau.MySlotmachine" >
				
			    <operators>
			        <operator>gt</operator>
			        <operator>lt</operator>
			    </operators>
			    <units>
			        <unit>°C</unit>
			        <unit>°F</unit>
			    </units>
				<values type="Float">
					<value>30</value>
					<value>25</value>
					<value>20</value>
					<value>15</value>
					<value>10</value>
					<value>5</value>
					<value>0</value>
					<value>-5</value>
					<value>-10</value>
					<value>-15</value>
					<value>-20</value>
				</values>
			</arg>
	</vocabulary>
	
	<vocabulary word="Regen" display-name="Regen" trigger-id="Rain" />
	
	<vocabulary display-prefix="alle" display-name="alle ... Minuten" trigger-id="TimeInterval">
        <arg name="Minutes"
            type="de.fuberlin.whitespace.regelbau.logic.data.TriggerArgument"
            description="Ausführungsintervall"
            selector="de.fuberlin.whitespace.regelbau.MyTimePicker" >
				
            <values type="Timespan" />
		</arg>
	</vocabulary>
	
	<vocabulary display-prefix="um" display-suffix="Uhr" display-name="um ... Uhr" trigger-id="TimeOfDay">
        <arg name="Minutes"
            type="de.fuberlin.whitespace.regelbau.logic.data.TriggerArgument"
            description="Ausführungsuhrzeit"
            selector="de.fuberlin.whitespace.regelbau.MyTimePicker" >
				
            <values type="Timestamp" />
		</arg>
	</vocabulary>
	
	<vocabulary display-prefix="nach" display-name="nach ... Minuten" trigger-id="TimeElapsed">
        <arg name="Minutes"
            type="de.fuberlin.whitespace.regelbau.logic.data.TriggerArgument"
            description="Zeit bis zur Ausführung"
            selector="de.fuberlin.whitespace.regelbau.MyTimePicker" >
				
            <values type="Timespan" />
		</arg>
	</vocabulary>
</trigger-vocabularies>